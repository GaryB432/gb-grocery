{
  "routes": [
    {
      "route": "/assets/*.{png,jpg,gif}",
      "headers": {
        "Cache-Control": "public, max-age=604800, immutable"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html"
  },
  "globalHeaders": {
    "X-Frame-Options": "DENY"
  },
  "mimeTypes": {
    ".json": "text/json",
    ".webmanifest": "application/manifest+json"
  }
}
